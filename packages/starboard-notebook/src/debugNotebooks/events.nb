runtime.dom.notebook.addEventListener("sb:change_cell_type", (evt) => {
    console.count("Evt type:", evt);
    if (evt.detail.newCellType === "python") {
        alert("I don't think so 🐍");
        evt.preventDefault();
    } else {
        alert("I know so");
    }
})
